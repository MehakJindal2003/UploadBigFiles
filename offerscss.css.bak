.offers{
	background-image:url("offerback.png");
}
.dealhead{
	display:flex;
	flex-direction:row;
}
.dealhead{
	padding-left:400px;
}
.deal1{
	font-size:45px;
	padding-top:35px;
	padding-left:60px;
	color:red;
}
.menuimage{
	width:90%;
	height:400px;
	margin-left:70px;
	border:4px solid black;
	border-radius:10px;
	margin-top:30px;
	margin-bottom:30px;
}
.combo{
	display:flex;
	flex-direction:row;
	margin:30px;
	margin-top:50px;
	border:5px solid black;
	border-radius:10px;
	padding-bottom:20px;
	outline:4px dashed #A020F0;
	outline-offset:5px;
	background-image:linear-gradient(to right,#CBC3E3,white,#CBC3E3,white,#CBC3E3);
}
.comboimage1{
	border:2px solid black;
	border-radius:50%;
	margin-top:50px;
	margin-left:20px;
	outline:3px dotted blue;
	outline-offset:5px;
}
.click{
	display:flex;
	flex-direction:row;
}
.clickme{
	font-size:30px;
	color:red;
	padding-left:220px;
	padding-top:50px;
	text-decoration:underline;
}
.fas{
	padding-top:40px;
	padding-left:20px;
	color:red;
}
.combo2{
	margin:30px;
	margin-top:20px;
	margin-left:60px;
}
.combo22{
	font-size:35px;
	color:red;
	text-decoration:underline;
}
.combo222{
	margin-left:80px;
}
.combo2222{
	font-size:35px;
	padding-top:20px;
}
.combo22222{
	font-size:40px;
	color:green;
	margin-top:20px;
}
.java2{
	font-size:45px;
	font-size:45px;
	padding-top:20px;
	font-family:fantasy;
}
.menu2{
	margin-top:90px;
}
.menu22{
	width:100%;
	border-radius:20px;
}
.discount{
	margin-top:100px;
}
.couponimage{
	width:100%;
	height:300px;
}
.discounthead{
	font-size:30px;
	padding-left:400px;
	text-decoration:underline;
	font-family:cursive;
	color:#8B0000;
	background-image:linear-gradient(to right,#FFDBE9,white,#FFDBE9,white,#FFDBE9,white,#FFDBE9,white,#FFDBE9);
}
.discountbox{
	display:flex;
	flex-direction:row;
	margin-top:20px;

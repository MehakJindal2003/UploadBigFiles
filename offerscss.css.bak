	height:550px;
}
.couponimage1,.couponimage2,.couponimage3{
	border-radius:20px;
}
.discountoffer{
	border-radius:20px;
	border:4px solid #A020F0;
	font-size:30px;
	height:130px;
	margin:20px;
	margin-top:30px;
	width:300px;
	background-color:#FFEFFF;
}
.food{
	color:red;
}
.boxoffer2{
	background-color:#90EE90;
	font-size:30px;
	border-radius:20px;
	border:3px solid blue;
	width:360px;
	height:60px;
	padding-left:20px;
	padding-bottom:20px;
}
.java{
	font-size:35px;
	font-family:fantasy;
